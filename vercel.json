{
    "version": 2,
    "builds": [
      {
        "src": "server-app/index.js", 
        "use": "@vercel/node"
      },
      {
        "src": "src/$1", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server-app/index.js" }, 
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "src/$1" } 
    ]
  }
  